alias :q='exit'
alias :wq='exit'
alias aliases="nvim ~/.aliases; refresh"
alias atr="aclocal; autoconf; automake --add-missing"
alias bsprc="nvim ~/.config/bspwm/bspwmrc"
alias cat="bat"
alias clip="xclip -selection clipboard"
alias clipout="xclip -o -selection clipboard"
alias db='dropbox-cli'
