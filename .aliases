alias governor="cat /sys/devices/system/cpu/cpu*/cpufreq/scaling_governor | head -1"
alias gpg-import="gpg --keyserver pool.sks-keyservers.net --recv-keys"
alias gt='gitui'
alias i3cfg="nvim ~/.config/i3/config"
alias l='exa -lbF --git'
alias la='exa -lbhHigUmuSa --time-style=long-iso --git --color-scale'
