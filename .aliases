alias zshrc="nvim ~/.zshrc; source ~/.zshrc"
alias zt="zathura"
alias docked="sudo system76-power charge-thresholds --profile max_lifespan"
alias charge="sudo system76-power charge-thresholds --profile full_charge"
alias pjson="python3 -m json.tool | bat --decorations never -l json"
alias t="taskwarrior-tui"
alias less="bat --paging always"
